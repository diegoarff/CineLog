{
  "cli": {
    "version": ">= 5.9.1"
  },
  "build": {
    "development": {
      "distribution": "internal",
      "android": {
        "gradleCommand": ":app:assembleDebug"
      },
      "ios": {
        "buildConfiguration": "Debug"
      }
    },
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://cinelog-backend-server.onrender.com/"
      }
    },
    "production": {
      "env": {
        "EXPO_PUBLIC_API_URL": "https://cinelog-backend-server.onrender.com/"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
